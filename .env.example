# Database Configuration
# Copy this file to .env and update with your actual database URL

# PostgreSQL connection string
# Format: postgres://username:password@host:port/database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/composable_rust

# Logging level
RUST_LOG=info,composable_rust=debug

# Test database (used by integration tests with testcontainers)
TEST_DATABASE_URL=postgres://postgres:postgres@localhost:5432/composable_rust_test
