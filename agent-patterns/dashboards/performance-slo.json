{
  "dashboard": {
    "title": "Agent Performance & SLOs",
    "tags": ["agents", "performance", "slo"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 9, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "Request Success Rate (SLO: 99.9%)",
        "targets": [
          {
            "expr": "sum(rate(agent_execution_duration_seconds_count{status=\"success\"}[5m])) / sum(rate(agent_execution_duration_seconds_count[5m])) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Success %",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "legend": {
          "show": true,
          "values": true,
          "current": true,
          "avg": true
        },
        "thresholds": [
          {
            "value": 99.9,
            "colorMode": "critical",
            "op": "lt",
            "fill": true,
            "line": true,
            "yaxis": "left"
          }
        ],
        "alert": {
          "name": "Success Rate Below SLO",
          "conditions": [
            {
              "evaluator": {
                "params": [99.9],
                "type": "lt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "message": "Agent success rate dropped below 99.9% SLO",
          "noDataState": "no_data"
        }
      },
      {
        "id": 2,
        "gridPos": { "h": 9, "w": 12, "x": 12, "y": 0 },
        "type": "graph",
        "title": "P99 Latency by Agent (SLO: <1s)",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(agent_execution_duration_seconds_bucket[5m])) by (le, agent_name))",
            "legendFormat": "p99 - {{agent_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "legend": {
          "show": true,
          "alignAsTable": true,
          "values": true,
          "current": true,
          "max": true
        },
        "thresholds": [
          {
            "value": 1,
            "colorMode": "critical",
            "op": "gt",
            "fill": false,
            "line": true,
            "yaxis": "left"
          }
        ],
        "alert": {
          "name": "P99 Latency Above SLO",
          "conditions": [
            {
              "evaluator": {
                "params": [1],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "max"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "message": "Agent P99 latency exceeded 1 second SLO",
          "noDataState": "no_data"
        }
      },
      {
        "id": 3,
        "gridPos": { "h": 8, "w": 6, "x": 0, "y": 9 },
        "type": "stat",
        "title": "Error Budget Remaining (30d)",
        "targets": [
          {
            "expr": "(1 - (sum(rate(agent_execution_duration_seconds_count{status=\"error\"}[30d])) / sum(rate(agent_execution_duration_seconds_count[30d])))) / 0.001 * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 25, "color": "yellow" },
                { "value": 50, "color": "green" }
              ]
            },
            "min": 0,
            "max": 100
          }
        },
        "pluginVersion": "10.0.0"
      },
      {
        "id": 4,
        "gridPos": { "h": 8, "w": 6, "x": 6, "y": 9 },
        "type": "stat",
        "title": "Total Requests (5m)",
        "targets": [
          {
            "expr": "sum(rate(agent_execution_duration_seconds_count[5m])) * 60 * 5",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {
              "mode": "palette-classic"
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": { "h": 8, "w": 6, "x": 12, "y": 9 },
        "type": "stat",
        "title": "Average Latency (5m)",
        "targets": [
          {
            "expr": "sum(rate(agent_execution_duration_seconds_sum[5m])) / sum(rate(agent_execution_duration_seconds_count[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 0.5, "color": "yellow" },
                { "value": 1, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": { "h": 8, "w": 6, "x": 18, "y": 9 },
        "type": "stat",
        "title": "Error Rate (5m)",
        "targets": [
          {
            "expr": "sum(rate(agent_execution_duration_seconds_count{status=\"error\"}[5m])) / sum(rate(agent_execution_duration_seconds_count[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 0.1, "color": "yellow" },
                { "value": 1, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 17 },
        "type": "graph",
        "title": "Latency Heatmap by Agent",
        "targets": [
          {
            "expr": "sum(rate(agent_execution_duration_seconds_bucket[5m])) by (le, agent_name)",
            "format": "heatmap",
            "legendFormat": "{{agent_name}} - {{le}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        }
      },
      {
        "id": 8,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 25 },
        "type": "graph",
        "title": "Tool Success Rate",
        "targets": [
          {
            "expr": "sum(rate(agent_tool_invocations_total{status=\"success\"}[5m])) by (tool_name) / sum(rate(agent_tool_invocations_total[5m])) by (tool_name) * 100",
            "legendFormat": "{{tool_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percent",
            "label": "Success %",
            "min": 0,
            "max": 100
          },
          {
            "format": "short"
          }
        ],
        "xaxis": {
          "show": true,
          "mode": "time"
        },
        "legend": {
          "show": true,
          "alignAsTable": true,
          "values": true,
          "current": true,
          "avg": true
        }
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 25 },
        "type": "table",
        "title": "SLO Compliance by Agent (Last 24h)",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, sum(rate(agent_execution_duration_seconds_bucket[24h])) by (le, agent_name))",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "sum(rate(agent_execution_duration_seconds_count{status=\"success\"}[24h])) by (agent_name) / sum(rate(agent_execution_duration_seconds_count[24h])) by (agent_name) * 100",
            "format": "table",
            "instant": true,
            "refId": "B"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true
              },
              "indexByName": {
                "agent_name": 0,
                "Value #A": 1,
                "Value #B": 2
              },
              "renameByName": {
                "agent_name": "Agent",
                "Value #A": "P99 Latency (s)",
                "Value #B": "Success Rate (%)"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "footer": {
            "show": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 1, "color": "red" }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Success Rate (%)"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "mode": "absolute",
                    "steps": [
                      { "value": null, "color": "red" },
                      { "value": 99, "color": "yellow" },
                      { "value": 99.9, "color": "green" }
                    ]
                  }
                }
              ]
            }
          ]
        }
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"]
    },
    "annotations": {
      "list": [
        {
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 1,
    "links": [],
    "liveNow": false,
    "panels": []
  },
  "overwrite": false
}
